df = pd.read_csv("data/processed/full_data.csv")
sample = df[df['OEM'] == 'OEM_A'].groupby('cell').get_group(0)

plt.plot(sample['capacity'], sample['feature10'])
plt.xlabel("Capacity (Ah)")
plt.ylabel("Feature10")
plt.title("Feature10 vs Capacity â€” Cell 0")
plt.grid()
plt.show()

